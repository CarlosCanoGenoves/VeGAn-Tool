<?xml version="1.0" encoding="UTF-8"?>
<goalModel:GoalModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:goalModel="http://goalModel"
    xsi:schemaLocation="http://goalModel goalModel.ecore"
    name="goalModel">
  <actors elementName="actor">
    <intentionalelements
        xsi:type="goalModel:Task"
        elementName="A">
      <srcLinks
          xsi:type="goalModel:Contribution"
          trgs="//@actors.0/@intentionalelements.1"
          contributionType="p50"/>
    </intentionalelements>
    <intentionalelements
        xsi:type="goalModel:Task"
        elementName="B"
        trgLinks="//@actors.0/@intentionalelements.0/@srcLinks.0">
      <srcLinks
          xsi:type="goalModel:Contribution"
          trgs="//@actors.0/@intentionalelements.2"
          contributionType="n50"/>
    </intentionalelements>
    <intentionalelements
        xsi:type="goalModel:Task"
        elementName="C"
        trgLinks="//@actors.0/@intentionalelements.1/@srcLinks.0">
      <srcLinks
          xsi:type="goalModel:Contribution"
          trgs="//@actors.0/@intentionalelements.3"
          contributionType="p50"/>
    </intentionalelements>
    <intentionalelements
        xsi:type="goalModel:Task"
        elementName="D"
        trgLinks="//@actors.0/@intentionalelements.2/@srcLinks.0"/>
  </actors>
</goalModel:GoalModel>
