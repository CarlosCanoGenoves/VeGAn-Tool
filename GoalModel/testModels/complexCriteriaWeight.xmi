<?xml version="1.0" encoding="UTF-8"?>
<goalModel:GoalModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:goalModel="http://goalModel"
    xsi:schemaLocation="http://goalModel goalModel.ecore"
    name="goalModel">
  <actors elementName="actor"
      importance="Very_High"
      confidence="Possibly_More">
    <intentionalelements
        xsi:type="goalModel:Task"
        elementName="T1"
        importance="Very_High"
        confidence="Possibly_More">
      <srcLinks
          xsi:type="goalModel:Decomposition"
          trgs="//@actors.0/@intentionalelements.1 //@actors.0/@intentionalelements.2"/>
    </intentionalelements>
    <intentionalelements
        xsi:type="goalModel:Task"
        elementName="T2"
        importance="Medium"
        confidence="Confident"
        trgLinks="//@actors.0/@intentionalelements.0/@srcLinks.0"/>
    <intentionalelements
        xsi:type="goalModel:Task"
        elementName="T3"
        importance="Very_Low"
        confidence="Possibly_Less"
        trgLinks="//@actors.0/@intentionalelements.0/@srcLinks.0"/>
  </actors>
</goalModel:GoalModel>
